{
  "name": "gobii-cli",
  "version": "1.3.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist && find src -name '*.js' -delete && find src -name '*.js.map' -delete",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "postbuild": "chmod +x dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:cli": "vitest run tests/cli"
  },
  "author": "Will Bonde",
  "license": "Apache-2.0",
  "description": "",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "execa": "^9.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "cli-spinners": "^3.2.0",
    "commander": "^14.0.0",
    "ora": "^8.2.0",
    "table": "^6.9.0"
  },
  "bin": {
    "gobii-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/willbonde/gobii-cli",
  "keywords": [
    "gobii",
    "cli",
    "browser",
    "automation",
    "ai",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willbonde/gobii-cli.git"
  }
}
